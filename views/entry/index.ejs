	<style>
		@media screen and (max-width:768px) {
			.blog-desc {
				margin-bottom: 15px;
			}
			.blog-item .blog-desc h4 , .blog-description-p {
				margin-left: 10px;
				margin-right: 10px;
			}
		}

		.social-icons {
			display: flex;
			justify-content: flex-start;
			margin-bottom: 20px;
			margin-top:40px;
		}

		.social-icons li a {
			margin-right: 5px;
		}
		.resume-button {
			margin-top: 20px;
		}

		.badge-container span {
			margin-right: 5px;
		}

		.blog-item-container {
			margin-top: 30px;
			box-shadow: 0 0 10px gray;
		}

		.blog-item-container .blog-img {
			margin-top: 0px!important;
		}

		.date-minute-container p{
			font-size: 12px!important;
			font-weight: 100;
		}
	</style>
</head>

<body>

	<div id="preloader">
		<div id="status">
			<div class="loader-revolve center">
				<span></span>
			</div>
		</div>
	</div>

	<div id="navigation">
		<div class="container">
			<div class="row">
				<div class="col-sm-3">
					<div class="logo">
						<a href="#home">Jimmy Tran</a>
					</div>
				</div>

				<div class="col-sm-9">
					<div class="navigation-menu">
						<div class="navbar">
							<div class="navbar-header">
								<button type="button" class="navbar-toggle" data-toggle="collapse"
									data-target=".navbar-collapse">
									<span class="sr-only">Toggle navigation</span>
									<span class="icon-bar"></span>
									<span class="icon-bar"></span>
									<span class="icon-bar"></span>
								</button>
							</div>

							<div class="navbar-collapse collapse">
								<ul class="nav navbar-nav navbar-right">
									<li><a class="active smoth-scroll" href="#home">Home</a></li>
									<li><a class="smoth-scroll" href="#about">About</a></li>
									<li><a class="smoth-scroll" href="#portfolio">Projects</a></li>
									<li><a class="smoth-scroll" href="#blog">Blog</a></li>
									<li><a class="smoth-scroll" href="#contact">Contact</a></li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div id="home" class="home-bg"
		style="background-image: url('https://www.wallpaperflare.com/static/471/767/754/abstract-ae-plexus-blue-dot-wallpaper.jpg')"
		data-stellar-background-ratio="0.6">
		<div class="overlay">
			<div class="container">
				<div class="row">
					<div class="home-text">
						<div class="profile-pic" data-aos="fade-zoom-in" data-aos-easing="ease-in-back"
							data-aos-delay="300" data-aos-offset="0">
							<img src="images/propic.jpg" />
						</div>

						<div data-aos="fade-zoom-in" data-aos-easing="ease-in-back" data-aos-delay="300"
							data-aos-offset="0">
							<h1>Hi, I'm Jimmy</h1>
							<h3 id="text-rotator">
								<div>Software Engineer</div>
								<div>Full Stack Developer</div>
							</h3>
						</div>
					</div>
					<a class="smoth-scroll" href="#about"><i class="fa fa-angle-down arrow"></i></a>
				</div>
			</div>
		</div>
	</div>
	<section id="about">
		<div class="container">
			<div class="row">
				<div style="width: 100%; padding:0px 30px;" class="description col-md-6 col-sm-4 col-xs-12">
					<h2>About Me</h2>
					<!-- <h4>Web Developer from Ireland</h4> -->
					<p>I am a Full Stack Software Engineer with a love for problem-solving and a passion for technology.
						I love creating meaningful projects that will make an immediate impact on the world. I am a
						problem solver, with quick abilities to learn and adapt, and I'm looking forward to bringing my
						skill set and knowledge to collaborate with a team and build high quality software.
					</p>
					<ul class="social-icons social-list">
						<!-- <li><a href="https://www.github.com/jimmytran16" target="_blank"><i class="fa fa-github fa-lg"></i></a></li>
						<li><a href="https://www.linkedin.com/in/jimmyviettran" target="_blank"><i class="fa fa-linkedin fa-lg"></i></a></li> -->
						<a href="https://www.github.com/jimmytran16" target="_blank" class="btn btn-dark"><i
							class="fa fa-github"></i></a>
						<a href="https://www.linkedin.com/in/jimmyviettran" target="_blank" class="btn btn-dark"><i
							class="fa fa-linkedin"></i></a>
						<a href="/resume/JimmyVTResume.pdf" target="_blank" class="btn btn-dark with-arrow smoth-scroll">My Resume
							<i class="fa fa-download"></i>
						</a>
					</ul>
					<p>
						<!-- <a href="/resume/JimmyVTResume.pdf" target="_blank" class="btn btn-dark with-arrow smoth-scroll resume-button">My Resume
							<i class="fa fa-download"></i>
						</a> -->
					</p>

				</div>
				</div>
			</div>
		</div>
		</div>
	</section>

	<section id="portfolio">
		<div class="container text-center">
			<div class="row">
				<div class="col-md-12 text-center">
					<h2>Recent Projects</h2>
					<% for (var i=0; i < data.length; i++) { %>
						<div class="col-md-4 col-sm-6">
							<div class="blog-item">
								<a href="<%= data[i]['endpoint'] %>" class="blog-img">
									<img src="<%= data[i]['image_path'] %>">
								</a>
								<div class="blog-desc">
									<h4>
										<a href="single.html">
											<%= data[i]['name'] %>
										</a>
									</h4>
									<p class="project-description-index">
										<%= data[i]['description'] %>
									</p>
									<a href="<%= data[i]['endpoint'] %>" class="btn btn-dark"><i
											class="fa fa-eye"></i></a>
								</div>
							</div>
						</div>
						<% } %>

				</div>
			</div>
	</section>

	<section id="blog">
		<div class="container text-center">
			<div class="row">
				<div class="col-md-12 text-center">
					<h2>Blog</h2>
				</div>
				<%
					// render the blogs
					for (var i = 0; i < blogs.length; i++) { %>
						<div class="col-md-4 col-sm-6 blog-info-container">
							<div class="blog-item blog-item-container">
								<a href="/blog/readmore/<%=blogs[i]['_id']%>" class="blog-img">
									<img src="<%=blogs[i]['img_path']%>">
								</a>
								<div class="blog-desc">
									<div>
										<h4>
											<a href="/blog/readmore/<%=blogs[i]['_id']%>"><%=blogs[i]['title']%></a>
										</h4>
										<p class="blog-description-p"><%=blogs[i]['description'].substring(0,85)%> ...</p>
										<div class="badge-container" style="display: flex;justify-content:center;padding:5px 12px;margin-bottom:10px;">
											<% 
												let tags = blogs[i]['tags']
												for (var x = 0; x < tags.length; x++) { %>
													<span class="badge badge-pill <%= tags[x][1] %>"> <%= tags[x][0] %> </span>
												<% } %>	
										</div>
									</div>
									<!-- <a href="single.html" class="btn btn-dark">Read more</a> -->
									<div class="date-minute-container">
										<p>
											<%= blogs[i]['date'].split(',')[0] %>  &bull;  <%= blogs[i]['minutes'] %> min read
										</p>
									</div>
								</div>
							</div>
						</div>
				<%
					}
				%>
				<%
					// if there are no blogs, then return a message
					if (blogs.length === 0) { %>
						<p>No posts yet ...</p>
				<%
					}
				%>

			</div>
		</div>
	</section>

	<section id="contact">
		<div class="container text-center">
			<div class="row">
				<div class="col-md-12 text-center">
					<h2>Contact Me</h2>
				</div>

				<div class="address-icons col-md-8 col-md-offset-2">
					<div class="col-md-4 col-sm-4">
						<i class="fa fa-envelope fa-2x"></i>
						<p>dew@email.com</p>
					</div>

					<div class="col-md-4 col-sm-4">
						<i class="fa fa-map-marker fa-2x"></i>
						<p>Banguntapan,
							<br> Yogyakarta, ID 55198
						</p>
					</div>

					<div class="col-md-4 col-sm-4">
						<i class="fa fa-phone fa-2x"></i>
						<p>+62 123 456 7890</p>
					</div>
					<div class="clearfix"></div>
				</div>

				<div class="col-md-8 col-md-offset-2">
					<!-- changes your email domain in mailer.php (assets/php/) -->
					<form id="ajax-contact" name="contact" method="post" action="assets/php/mailer.php">
						<div class="row">
							<div class="col-md-6 col-sm-6">
								<div class="form-group">
									<input type="text" name="name" id="name" class="form-control" placeholder="Name"
										required>
									<p class="help-block text-danger"></p>
								</div>
							</div>
							<div class="col-md-6 col-sm-6">
								<div class="form-group">
									<input type="email" name="email" id="email" class="form-control" placeholder="Email"
										required>
									<p class="help-block text-danger"></p>
								</div>
							</div>
						</div>
						<div class="form-group">
							<textarea name="message" id="message" class="form-control" rows="4" placeholder="Message..."
								required></textarea>
							<p class="help-block text-danger"></p>
						</div>
						<div class="col-md-12 col-xs-12">
							<button id="submit" type="submit" name="submit" class="btn btn-dark">Send Message</button>
						</div>
						<div id="form-messages" class="col-md-12 col-xs-12"></div>
					</form>
				</div>
			</div>
		</div>
	</section>
	<!-- //CONTACT END-->

	<footer id="footer">
		<div class="container">
			<div class="col-md-12 text-center">
